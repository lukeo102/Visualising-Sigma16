     lea    R1,6[R0]      ; R1 := 6
     load   R2,x[R0]      ; R2 := x (variable initialized to 7)
     mul    R3,R1,R2      ; R3 := 6 * x = 42 (hex 002a)
     store  R3,result[R0] ; result := 6 * x
     trap   R0,R0,R0      ; halt
x         data   7         ; initial value of x = 7
result    data   0         ; initial value of result = 0
